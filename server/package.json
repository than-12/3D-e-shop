{
  "name": "server",
  "version": "1.0.0",
  "description": "3D PrintCraft Server API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "set NODE_ENV=development&& nodemon src/index.ts",
    "build": "tsc",
    "seed": "ts-node db/seed.ts",
    "studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "tsx db/index.ts",
    "db:seed": "tsx db/seed.ts",
    "db:create-tables": "tsx db/create-tables.ts"
  },
  "keywords": [
    "3d",
    "printing",
    "ecommerce",
    "api"
  ],
  "author": "3D PrintCraft Team",
  "license": "ISC",
  "dependencies": {
    "@libsql/client": "^0.15.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.9.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.28.5",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.10",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^6.4.14",
    "drizzle-kit": "^0.19.13",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
